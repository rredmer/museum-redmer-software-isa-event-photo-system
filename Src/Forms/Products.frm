Private Sub tlbProducts_ButtonClick(ByVal Button As MSComctlLib.Button)
    On Error Resume Next
    Select Case Button.Index
        Case 1      'Exit
            Me.Hide
        Case 2
            dbgProducts.AllowUpdate = True
        Case 3
            MsgBox "Feature not available"
        Case 4
            
            If MsgBox("Are you sure?", vbApplicationModal + vbQuestion + vbYesNo, "Delete Subject") = vbYes Then
               DE.rscmdSelectProductCodes.Delete adAffectCurrent
            End If

    End Select

End Sub
